@model List<EducationManagementStudio.Models.CourseModels.Course>

<h1>Courses List</h1>
<p class="lead">List of courses</p>
<hr />

<table class="table table-striped table-hover table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Creator</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Name</td>
                <td>@course.Creator.FullName</td>
            </tr>
        }
    </tbody>
</table>

